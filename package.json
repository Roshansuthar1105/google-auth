{
  "name": "google-auth-app",
  "version": "1.0.0",
  "description": "MERN Authentication App with Google OAuth",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "server1": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "mern",
    "authentication",
    "google-oauth",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
